{
    "manifest_version": 3,
    "name": "LeetCode Submission Tracker",
    "version": "1.0",
    "description": "Track your LeetCode submissions",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/problems/*"],
        "js": ["content.js"]
      }
    ]
  }
  